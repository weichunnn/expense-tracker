  "bugs": {
    "url": "https://github.com/weichunnn/expense-tracker/issues"
  },
  "homepage": "https://github.com/weichunnn/expense-tracker#readme",
  "dependencies": {
    "color": "^3.1.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
